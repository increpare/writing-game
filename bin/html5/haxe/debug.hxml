-main ApplicationMain 
-cp /usr/local/lib/haxelib/openfl/3,5,3
-D openfl=3.5.3
-cp /usr/local/lib/haxelib/lime/2,8,2
-D lime=2.8.2
-cp src
-D openfl-next
-D tools=2.8.2
-D no-compilation
-D lime-opengl
-D openfl-html5
-D canvas
-D lime-html5
-D html5
-D web
--remap flash:openfl
-js bin/html5/bin/drawingimages.js
-cp bin/html5/haxe
-D html5
--macro allowPackage("flash")
-debug